<div class="list infinity-container">
  <ul>
    {{#each this.model as |val|}}
      <div class="infinity-svg">
        <svg width="200" height="250" version="1.1" xmlns="http://www.w3.org/2000/svg">
          {{{val}}}
        </svg>
      </div>
    {{/each}}
  </ul>
  <MyComponent
    class="infinity-scrollable"
    @scrollableAreaOverride=".list"
    @viewportSpyOverride={{true}}
    @viewportToleranceOverride={{this.viewportToleranceOverride}}
    @infinityLoad={{this.infinityLoad}}
  />
</div>
